{
  "name": "ngx-covalent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build --prod",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "serve": "npm run ng serve -o",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run write:env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "docs": "hads ./docs -o",
    "compodoc": "compodoc -p tsconfig.json -d docs/generated -s -o",
    "write:env": "ngx-scripts env npm_package_version",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --release --copy dist",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^11.2.10",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "^11.2.10",
    "@angular/compiler": "^11.2.10",
    "@angular/core": "^11.2.10",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.10",
    "@angular/localize": "^11.2.10",
    "@angular/material": "^11.2.9",
    "@angular/platform-browser": "^11.2.10",
    "@angular/platform-browser-dynamic": "^11.2.10",
    "@angular/router": "^11.2.10",
    "@angular/service-worker": "^11.2.10",
    "@capacitor/android": "2.4.7",
    "@capacitor/angular": "^1.0.3",
    "@capacitor/core": "latest",
    "@covalent/core": "^3.1.0",
    "@covalent/dynamic-forms": "^3.1.0",
    "@covalent/echarts": "^3.1.0",
    "@covalent/highlight": "^3.1.0",
    "@covalent/http": "^3.1.0",
    "@covalent/markdown": "^3.1.0",
    "@ionic-native/core": "^5.32.1",
    "@ionic-native/keyboard": "^5.32.1",
    "@ionic-native/splash-screen": "^5.32.1",
    "@ionic-native/status-bar": "^5.32.1",
    "@ngx-translate/core": "^13.0.0",
    "cordova-android": "^9.1.0",
    "cordova-custom-config": "^5.1.0",
    "cordova-ios": "^6.1.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "date-fns": "^2.21.1",
    "echarts": "^5.1.0",
    "lodash": "^4.17.20",
    "material-design-icons-iconfont": "^6.1.0",
    "material-icons": "^0.5.5",
    "moment": "^2.29.1",
    "ngx-mat-select-search": "^3.3.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.3",
    "zrender": "^5.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "^11.2.10",
    "@angular/language-service": "^11.2.10",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@capacitor/cli": "latest",
    "@compodoc/compodoc": "^1.1.11",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngx-rocket/scripts": "^5.0.0",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.20.10",
    "codelyzer": "^6.0.1",
    "cordova": "^10.0.0",
    "hads": "^3.0.0",
    "html-webpack-plugin": "^5.3.1",
    "htmlhint": "^0.14.2",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~7.0.0",
    "stylelint": "~13.8.0",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-custom-config": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  }
}
